package ch05.sec05;

public class CharAtExample {

	public static void main(String[] args) {
		String ssn = "9506241230123";
					 //0123455...
		char sex = ssn.charAt(3); //인덱스 번호로 해당 문자를 구한다.
		
		System.out.println(sex);
		
		switch (sex) {
		case '1':
		case '3':
			System.out.println("남자");
			break;
			
		case '2':
		case '4':
			System.out.println("여자");
			break;
		}
	}

}

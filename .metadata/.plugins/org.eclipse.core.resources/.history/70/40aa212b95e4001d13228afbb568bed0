package ch06.sec10.exam03;

public class Car {
	//인스턴스 필드
	int speed;
	//정적 필드
	static int gas;
	
	//인스턴스 메소드
	void run() {
		System.out.println(speed + "으로 달립니다.");
		System.out.println("가스양: "+ gas);
	}
	
	//정적static 메소드
	static void simulate() {
		
	}

	//리턴 타입이 없는 정적 메소드
	public static void main(String[] args) {
		
	}

}

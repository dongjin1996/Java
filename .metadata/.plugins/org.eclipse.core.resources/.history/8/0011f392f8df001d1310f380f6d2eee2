package ch04.sec02;

public class IfnestedExample {

	public static void main(String[] args) {
		//공식 int num = (int) (Math.random() " n ) + start;
		// n: 정수의 갯수, start: 시작 숫자
		// 
		
		
		int score = (int) (Math.random() * 45) + 1; //0.0 ~ 1.0 사이의 숫자를 만들어준다
		System.out.println(score);
		
		
	}

}

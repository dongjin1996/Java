package ch06.sec10.exam01;

public class CalculatorExample {

	public static void main(String[] args) {
		Calculator.pi = 4;
		Calculator myCalc1 = new Calculator();
		System.out.println("myCalc.pi " + myCalc1.pi);
		
		Calculator myCalc2 = new Calculator();
		System.out.println("myCalc.pi " + myCalc2.pi);
		
		Calculator myCalc3 = new Calculator();
		System.out.println("myCalc.pi " + myCalc3.pi);
	}

}

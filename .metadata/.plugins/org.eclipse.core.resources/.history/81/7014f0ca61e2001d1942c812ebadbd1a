package ch05.sec06;

public class ArrayCreateByValueListExample {

	public static void main(String[] args) {
		
		//1.초기데이터랑 같이 선언.
		String[] season = {"봄", "여름", "가을", "겨울"};
		
		//2. 배열타입, 배열의 크기만 선언
		String[] season2 = new String[4];
		season2[0] = "봄";
		season2[1] = "여름";
		season2[2] = "가을";
		season2[3] = "겨울";
		
		//3. 배열타입, 초기데이터랑 같이 선언
		String[] season3 = new String[]  {"봄", "여름", "가을", "겨울"};
		
	}

}

package ch02.sec09;

public class OperationPromotionExample {

	public static void main(String[] args) {
		byte v1 = 10;
		byte v2 = 20;
		int result2 = v1 + v2;
		
		byte v3 = 10;
		int v4 = 100;
		long v5 = 1000L;
		long result3 = v3 + v4 + v5;
		
		char v6 = 'A';
		char v7 = 1;
		int result4 = v6 + v7;
		
		
		
	}

}
